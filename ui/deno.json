{
  "name": "@stewpot/ui",
  "version": "0.0.0",
  "license": "MIT",
  "tasks": {
    "dev": "deno serve --watch=templates/,core/,components/ -R serve.ts",
    "test": "deno test -RW"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/http": "jsr:@std/http@1",
    "@std/testing": "jsr:@std/testing@^1.0.16",
    "marked": "npm:marked@^17.0.1",
    "ventojs/vento": "https://cdn.jsdelivr.net/gh/ventojs/vento@2.3/mod.ts"
  },
  "exports": {
    ".": "./main.ts"
  },
  "publish": {
    "exclude": [
      "./static",
      "./templates",
      "./tmp",
      "./__snapshots__",
      "*_test.ts",
      "./serve.ts"
