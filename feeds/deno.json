{
  "name": "@stewpot/feeds",
  "description": "Feed utilities and CLI",
  "version": "0.10.0",
  "license": "MIT",
  "exports": {
    ".": "./main.ts",
    "./cli": "./cli.ts"
  },
  "imports": {
    "@stewpot/html": "jsr:@stewpot/html@^0.1.0",
    "@std/assert": "jsr:@std/assert@^1.0.17",
    "@std/cli": "jsr:@std/cli@^1.0.26",
    "@std/fmt": "jsr:@std/fmt@^1.0.9",
    "@std/fs": "jsr:@std/fs@^1.0.22",
    "@std/path": "jsr:@std/path@^1.1.4",
    "@std/testing": "jsr:@std/testing@^1.0.17",
    "@std/ulid": "jsr:@std/ulid@^1.0.0",
    "feedsmith": "npm:feedsmith@^2.9.0"
  },
  "publish": {
    "exclude": [
      "*_test.ts",
      "fixtures",
      "__snapshots__"
    ]
  },
  "tasks": {
    "dev": {
      "description": "Run CLI reader application for development",
      "command": "deno -RWEN --watch=assets cli.ts reader"
    }
  },
  "compilerOptions": {
    "checkJs": true,
    "lib": [
      "deno.window",
      "deno.unstable",
      "dom"
    ]
  }
}
